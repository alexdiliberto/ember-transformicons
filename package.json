{
  "name": "ember-transformicons",
  "version": "7.0.0",
  "description": "Transformicons for Ember: Animated icons, symbols and buttons",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "transformicon",
    "navicon",
    "icon",
    "transform",
    "animation"
  ],
  "repository": "https://github.com/alexdiliberto/ember-transformicons",
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://alexdiliberto.github.io/ember-transformicons",
  "bugs": {
    "url": "https://github.com/alexdiliberto/ember-transformicons/issues"
  },
  "scripts": {
    "analyze": "broccoli-concat-analyser ./concat-stats-for",
    "build": "ember build",
    "build:prod": "ember build -prod",
    "build:prod:stats": "CONCAT_STATS=true yarn build:prod",
    "clean": "rm -rf dist tmp node_modules concat-stats-for",
    "changelog": "lerna-changelog",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:dependency": "ember dependency-lint",
    "lint:types": "tsc --skipLibCheck",
    "release": "release-it",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start:inspect": "node --inspect-brk yarn start",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "@glimmer/component": "^1.0.4",
    "ember-auto-import": "^1.11.3",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1",
    "ember-cli-typescript": "^4.2.0",
    "ember-decorators": "^6.1.1",
    "ember-truth-helpers": "^3.0.0"
  },
  "devDependencies": {
    "@ember/edition-utils": "^1.2.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.8",
    "@embroider/test-setup": "^0.41.0",
    "@types/ember": "^3.16.5",
    "@types/ember-qunit": "^3.4.13",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__application": "^3.16.2",
    "@types/ember__array": "^3.16.4",
    "@types/ember__component": "^3.16.4",
    "@types/ember__controller": "^3.16.4",
    "@types/ember__debug": "^3.16.3",
    "@types/ember__engine": "^3.16.2",
    "@types/ember__error": "^3.16.1",
    "@types/ember__object": "^3.12.5",
    "@types/ember__polyfills": "^3.12.1",
    "@types/ember__routing": "^3.16.14",
    "@types/ember__runloop": "^3.16.3",
    "@types/ember__service": "^3.16.1",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^3.16.1",
    "@types/ember__test": "^3.16.1",
    "@types/ember__test-helpers": "^2.0.0",
    "@types/ember__utils": "^3.16.2",
    "@types/htmlbars-inline-precompile": "^1.0.1",
    "@types/qunit": "^2.11.1",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-concat-analyser": "^5.0.0",
    "ember-cli": "~3.26.1",
    "ember-cli-addon-docs": "^3.0.0",
    "ember-cli-addon-docs-yuidoc": "^1.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-dependency-lint": "^2.0.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.3.4",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-favicon": "^2.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-data": "^3.27.1",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-open-browser": "^1.0.0",
    "ember-percy": "1.6.0",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "~3.27.5",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.4.2",
    "ember-try": "^1.4.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-qunit": "^6.1.1",
    "eslint-plugin-unicorn": "^32.0.1",
    "lerna-changelog": "^1.0.1",
    "loader.js": "4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0",
    "release-it": "^14.9.0",
    "release-it-lerna-changelog": "^3.1.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": "12.* || >= 14"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://alexdiliberto.com/ember-transformicons",
    "versionCompatibility": {
      "ember": ">=3.16"
    }
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    }
  },
  "changelog": {
    "ignoreCommitters": [
      "dependabot"
    ]
  },
  "volta": {
    "node": "14.17.1",
    "yarn": "1.22.10"
  }
}
