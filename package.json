{
  "name": "ember-transformicons",
  "version": "2.0.1",
  "description": "Transformicons addon for your Ember CLI project",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "transformicon",
    "navicon",
    "icon",
    "transform",
    "animation"
  ],
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexdiliberto/ember-transformicons"
  },
  "bugs": {
    "url": "https://github.com/alexdiliberto/ember-transformicons/issues"
  },
  "scripts": {
    "postinstall": "npm rebuild node-sass",
    "build": "ember build",
    "start": "ember serve",
    "test": "ember try:each",
    "mocha": "cross-env DEBUG=ember-cli-addon-tests mocha",
    "test:all": "yarn test && mocha",
    "test:iterate": "ember exam:iterate 10",
    "start:inspect": "node --inspect --debug-brk node_modules/.bin/ember s",
    "build:stats": "CONCAT_STATS=true yarn build",
    "lint": "eslint addon app test tests *.js",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "lint:fix": "yarn lint -- --fix"
  },
  "dependencies": {
    "ember-cli-babel": "^6.9.2",
    "ember-cli-htmlbars": "^2.0.2"
  },
  "devDependencies": {
    "@alexdiliberto/eslint-config": "^1.1.1",
    "broccoli-asset-rev": "^2.4.5",
    "chai": "^4.1.1",
    "chalk": "^2.3.0",
    "cross-env": "^5.1.1",
    "denodeify": "^1.2.1",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.18.0",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-anybar": "0.5.0",
    "ember-cli-app-version": "^3.1.0",
    "ember-cli-content-security-policy": "1.0.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-dependency-lint": "1.0.2",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.0",
    "ember-cli-eslint": "^4.2.2",
    "ember-cli-github-pages": "0.1.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.2",
    "ember-cli-inject-live-reload": "^1.7.0",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-sass": "^7.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-exam": "^1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-native-dom-helpers": "^0.6.0",
    "ember-open-browser": "^0.5.1",
    "ember-percy": "1.2.22",
    "ember-resolver": "^4.3.0",
    "ember-source": "~2.18.0",
    "eslint": "^4.11.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "4.6.0",
    "mocha": "^4.0.1",
    "request": "^2.81.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://alexdiliberto.com/ember-transformicons",
    "versionCompatibility": {
      "ember": ">=1.12.2"
    }
  }
}