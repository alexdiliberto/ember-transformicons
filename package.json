{
  "name": "ember-transformicons",
  "version": "3.0.2",
  "description": "Transformicons for Ember: Animated icons, symbols and buttons",
  "keywords": [
    "ember-addon",
    "ember",
    "ember-cli",
    "transformicon",
    "navicon",
    "icon",
    "transform",
    "animation"
  ],
  "repository": "https://github.com/alexdiliberto/ember-transformicons",
  "license": "MIT",
  "author": "Alex DiLiberto <npm@alexdiliberto.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "homepage": "https://alexdiliberto.github.io/ember-transformicons",
  "bugs": {
    "url": "https://github.com/alexdiliberto/ember-transformicons/issues"
  },
  "scripts": {
    "build": "ember build",
    "build:stats": "CONCAT_STATS=true yarn build",
    "changelog": "git-chglog -o CHANGELOG.md",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each && ember fastboot:test",
    "lint:fix": "yarn lint -- --fix",
    "start:inspect": "node --inspect-brk node_modules/.bin/ember s",
    "test:iterate": "ember exam:iterate 10",
    "test:fastboot": "DEBUG=ember-cli-addon-tests ember fastboot:test",
    "version": "git-chglog $(git describe --tags $(git rev-list --tags --max-count=1)) && yarn changelog && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && rm -rf tmp dist"
  },
  "dependencies": {
    "@ember-decorators/babel-transforms": "^5.1.3",
    "ember-auto-import": "^1.2.21",
    "ember-cli-babel": "^7.1.2",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-decorators": "^5.1.3",
    "ember-legacy-class-shim": "^1.0.5",
    "lodash.defaultto": "^4.14.0"
  },
  "devDependencies": {
    "@alexdiliberto/eslint-config": "^3.0.0",
    "@ember/optional-features": "^0.7.0",
    "babel-eslint": "^8.0.0",
    "broccoli-asset-rev": "^2.7.0",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.1",
    "broccoli-source": "^1.1.0",
    "chai": "^4.2.0",
    "ember-angle-bracket-invocation-polyfill": "^1.2.5",
    "ember-cli": "~3.7.1",
    "ember-cli-addon-docs": "^0.6.4",
    "ember-cli-addon-docs-esdoc": "^0.2.1",
    "ember-cli-addon-tests": "^0.11.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-dependency-lint": "^1.1.3",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-fastboot": "^2.0.4",
    "ember-cli-favicon": "^2.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-exam": "^2.0.3",
    "ember-export-application-global": "^2.0.0",
    "ember-fastboot-addon-tests": "^0.5.1",
    "ember-fetch": "^6.4.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-open-browser": "^1.0.0",
    "ember-percy": "1.5.0",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.8.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^7.0.1",
    "loader.js": "4.7.0",
    "qunit-dom": "^0.8.4"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://alexdiliberto.com/ember-transformicons",
    "versionCompatibility": {
      "ember": ">=1.12.2"
    }
  }
}
